<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.mohann.covid19.bottomnavigation.ui.home.HomeFragment"
        android:label="@string/title_india"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_who"
        android:name="com.mohann.covid19.bottomnavigation.ui.who.WhoFragment"
        android:label="@string/title_who"
        tools:layout="@layout/fragment_who" />

    <fragment
        android:id="@+id/navigation_resources"
        android:name="com.mohann.covid19.bottomnavigation.ui.resources.ResourcesFragment"
        android:label="@string/title_resources"
        tools:layout="@layout/fragment_resources" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.mohann.covid19.bottomnavigation.ui.profile.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profile" />

</navigation>
